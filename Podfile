  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TextCodeRecognizer
  pod 'Firebase/CoreOnly', '~> 5.17.0'
  pod 'Firebase/MLVision', '~> 5.17.0'
  pod 'Firebase/MLVisionTextModel', '~> 5.17.0'

  target 'TextCodeRecognizerTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TextCodeRecognizerUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
